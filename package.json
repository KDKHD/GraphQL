{
  "name": "graphql_template",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon start.ts",
    "build": "rimraf ./build && tsc",
    "prod": "node build/start.js"
  },
  "author": "Kenneth Kreindler",
  "license": "ISC",
  "_moduleAliases": {
    "@models/*": "build/models/*",
    "@modules/*": "build/graphQL/modules/*",
    "@graphQL/*": "build/graphQL/*",
    "@controllers/*": "build/controllers/*",
    "@routes/*": "build/routes/*",
    "@utils/*": "build/utils/*",
    "@validators/*": "build/validators/*",
    "@schemas/*": "build/schemas/*",
    "@services/*": "build/controllers/services/*",
    "@middlewares/*": "build/controllers/services/middlewares/*",
    "@config/*": "build/config/*",
    "@queries/*": "build/queries/*",
    "@customTypes/*": "build/types/*",
    "@root/*": "build/*"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.17.9",
    "@types/object-hash": "^2.1.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^7.14.11",
    "@types/pg-format": "^1.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prisma": "^2.29.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^2.29.1",
    "apollo-server-express": "^3.1.2",
    "bcrypt": "^5.0.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-modules": "^1.4.4",
    "graphql-tools": "^4.0.8",
    "joi": "^17.4.2",
    "module-alias": "^2.2.2",
    "object-hash": "^2.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "twilio": "^3.67.0",
    "yup": "^0.32.9"
  }
}
